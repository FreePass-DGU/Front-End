<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/img1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/back"
        android:layout_marginStart="23.19dp"
        android:layout_marginTop="19.91dp"
        android:contentDescription="TODO" />

    <ImageView
        android:id="@+id/img2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/mypage"
        android:layout_marginStart="323.19dp"
        android:layout_marginTop="17.69dp"
        android:contentDescription="TODO" />

    <TextView android:text="프리패스"
        android:textSize="30sp"
        android:id="@+id/textfreepass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="155.72dp"
        android:layout_marginTop="25.92dp"
        android:textColor="#707070"
        android:layout_centerHorizontal="@+id/button1"/>

    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="83dp"
        android:background="#707070"/>

    <Button android:text="가벼운 산책 코스"
        android:id="@+id/button1"
        android:layout_width="345dp"
        android:layout_height="84dp"
        android:textColor="#707070"
        android:background="@drawable/btn_bg"
        android:layout_centerInParent="true"/>

    <Button android:text="데이트 코스"
        android:id="@+id/button2"
        android:layout_width="345dp"
        android:layout_height="84dp"
        android:background="@drawable/btn_bg"
        android:textColor="#707070"
        android:layout_below="@+id/button1"
        android:layout_alignLeft="@+id/button1"/>

    <Button android:text="가족 등반 코스"
        android:id="@+id/button3"
        android:layout_width="345dp"
        android:layout_height="84dp"
        android:background="@drawable/btn_bg"
        android:textColor="#707070"
        android:layout_below="@+id/button2"
        android:layout_alignLeft="@+id/button2"/>

</RelativeLayout>